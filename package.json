{
  "name": "@lifeprompt/acli",
  "version": "0.7.1",
  "type": "module",
  "description": "acli - Agent CLI protocol for AI agents on MCP",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "bin": {
    "acli": "./dist/cli.js"
  },
  "files": [
    "dist",
    "AGENTS.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mcp",
    "ai",
    "agent",
    "cli",
    "llm",
    "model-context-protocol",
    "claude",
    "tool-definition",
    "context-window",
    "typescript"
  ],
  "author": "lifeprompt-team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeprompt-team/acli"
  },
  "homepage": "https://github.com/lifeprompt-team/acli#readme",
  "bugs": {
    "url": "https://github.com/lifeprompt-team/acli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.23.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@modelcontextprotocol/sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "zod": "^3.24.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
